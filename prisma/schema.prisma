generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Student {
  id Int @id @default(autoincrement())
  name String
  rollno String @unique
  branch String
  year String
  email String
  contact String 
  firstDomain String
  secondDomain String
  motivation String
  toContribute String
  experience String?
  additionalInfo String?
}
